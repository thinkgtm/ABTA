  <script type="text/javascript">
  $(function() {
    if ($(".accordion__header__extra").hasClass("accordion__active__extra")) {
      $(".accordion__header__extra").each(function() {
        if ($(this).hasClass('accordion__active__extra')) {
          $(this).parent().find(".accordion__body__extra").slideToggle();
        }
      })
    }
    $(".accordion__header__extra").click(function(e) {
      var openAccordion = $(this).closest(".accordion__item__extra").find(".accordion__body__extra");
      $(this).closest(".accordion__extra").find(".accordion__body__extra").not(openAccordion).slideUp();
      if ($(this).hasClass("accordion__active__extra")) {
        $(this).removeClass("accordion__active__extra");
      } else {
        $(this).closest(".accordion__extra").find(".accordion__header__extra.accordion__active__extra").removeClass("accordion__active__extra");
        $(this).addClass("accordion__active__extra");
      }
      openAccordion.stop(false, true).slideToggle();
      e.preventDefault();
    });
  });

  function getCookieAgent() {
    var dc = document.cookie;
    var prefix = "AGENT_DETAILS=";
    var begin = dc.indexOf("; " + prefix);
    if (begin == -1) {
      begin = dc.indexOf(prefix);
      if (begin != 0) return null;
    } else {
      begin += 2;
      var end = document.cookie.indexOf(";", begin);
      if (end == -1) {
        end = dc.length;
      }
    }
    return decodeURI(dc.substring(begin + prefix.length, end));
  }
  var setAgentCookie = () => {
    var currDate = new Date();
    currDate.setHours(currDate.getHours() + 24);
    var ExpirationDate = currDate.toUTCString();
    const aux = window.location.href.toString().replaceAll("#", ""); //scape the # to make the params accessible
    const urlParams = new URL(aux).searchParams;
    const decodeValue = (value) => (urlParams.get(value) ? (decodeURI(urlParams.get(value)).replaceAll('+', ' ')) : '')
    if (decodeValue('AXACENTER') || decodeValue('referrerid')) {
      const agentData = {}
      agentData.HPID = decodeValue('HPID'); // 80066800 ++ ADD Quote Rquest
      agentData.AxaCenter = decodeValue('AXACENTER'); // 5002001421 ++ ADD Quote Rquest
      agentData.street = decodeValue('VM_STR_HSNR'); // Richard-Zanders-Str. 
      agentData.firstname = decodeValue('VM_VORNAME'); // 
      agentData.location = decodeValue('VM_ORT'); // Bergisch Gladbach (lugar) // direccion 3
      agentData.faxvor = decodeValue('VM_FA_FAXVOR'); // 02202
      agentData.tel = decodeValue('VM_FA_TEL'); // 936400
      agentData.email = decodeValue('VM_EMAIL'); //grossbach.gmbh@axa.de
      agentData.telvor = decodeValue('VM_FA_TELVOR'); // 02202
      agentData.fax = decodeValue('VM_FA_FAX'); // 9364050
      agentData.surname = decodeValue('VM_NACHNAME'); // oficina de  GroÃbach und Colleagues GmbH
      agentData.plz = decodeValue('VM_PLZ'); // 51465 - direccion 2
      agentData.address = decodeValue('VM_ANREDE'); // 9
      agentData.page = decodeValue('VM_HOMEPAGE'); // https://www.axa-betreuer.de/grossbach.gmbh
      agentData.actionCode = decodeValue('AKTIONSCODE'); // 94008Q
      agentData.intermediaryCode = decodeValue('referrerid'); // ++ ADD Quote Rquest
      const agentDataString = JSON.stringify(agentData)
      document.cookie = "AGENT_DETAILS=" + agentDataString + "; expires=" + ExpirationDate + "; path=/";
    }
  }

  const shouldRenderBanner = (cookieData) => {
    if (cookieData && Object.keys(cookieData).length > 0) {
      for (const key in cookieData) {
        if (key !== 'intermediaryCode' && cookieData[key].trim() !== '') {
          return true;
        }
      }
      return false;
    }
    return false;
  }

  var setInfoIntoHTML = (cookieData) => {
    if (cookieData.email) {
      document.getElementById('extraDetailsEmail').textContent = cookieData.email;
      document.getElementById('extraDetailsEmailContainer').className = "content__item";
      document.getElementById('extraDetailsEmailMobile').textContent = cookieData.email;
      document.getElementById('extraDetailsEmailMobileContainer').className = "content__item";
      document.getElementById('contentDividerEmail').className = "content__divider";
    }
    if (cookieData.location || cookieData.street || cookieData.plz) {
      document.getElementById('extraDetailsNachname').textContent = cookieData.street + ', ' + cookieData.plz + ' ' + cookieData.location;
      document.getElementById('extraDetailsNachnameContainer').className = "content__item";
      document.getElementById('extraDetailsNachnameMobile-first').textContent = cookieData.street;
      document.getElementById('extraDetailsNachnameMobile-second').textContent = cookieData.plz + ' ' + cookieData.location;
      document.getElementById('extraDetailsNachnameFirstMobileContainer').className = "content__item";
      document.getElementById('extraDetailsNachnameSecondMobileContainer').className = "content__item";
    }
    if (cookieData.tel) {
      document.getElementById('extraDetailsTel').textContent = cookieData.telvor + ' ' + cookieData.tel;
      document.getElementById('extraDetailsTelContainer').className = "content__item";
      document.getElementById('extraDetailsTelMobile').textContent = cookieData.telvor + ' ' + cookieData.tel;
      document.getElementById('extraDetailsTelMobileContainer').className = "content__item";
      document.getElementById('contentDividerTel').className = "content__divider";
    }
    if (cookieData.page) {
      document.getElementById('extraDetailsLocation').textContent = cookieData.page;
      document.getElementById('extraDetailsLocationContainer').className = "content__item";
      document.getElementById('extraDetailsLocationMobile').textContent = cookieData.page;
      document.getElementById('extraDetailsLocationMobileContainer').className = "content__item";
      document.getElementById('contentDividerLocation').className = "content__divider";
    }
    if (cookieData.firstname || cookieData.surname) {
      document.getElementById('extraDetailsAgentNameMobile').textContent = cookieData.firstname + ' ' + cookieData.surname;
      document.getElementById('extraDetailsAgentNameMobileContainer').className = "content__item";
      document.getElementById('extraDetailsAgentName').textContent = cookieData.firstname + ' ' + cookieData.surname;
      document.getElementById('extraDetailsAgentNameContainer').className = "content__item";
    }
  }
  window.addEventListener('DOMContentLoaded', function() {
    setAgentCookie()
    const dataExtraDetails = JSON.parse(getCookieAgent());
    if (shouldRenderBanner(dataExtraDetails)) {
      document.getElementById('extra-details-container').className = "extra-details-v2"
      document.getElementById('extra-details-container-mobile').className = "extra-details-v2--mobile accordion"
      setInfoIntoHTML(dataExtraDetails)
    }
  });
  </script>	<!-- NAVIGATION MOBILE-->
