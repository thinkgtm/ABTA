    window.addEventListener('DOMContentLoaded', function() {
        function equalizeHeights() {
            const headers = document.querySelectorAll('.card-v2__content__header');
            let maxHeight = 0;
            if(headers && headers.length > 1) {
                headers.forEach(function(header) {
                const height = header.offsetHeight;
                if (height > maxHeight) {
                    maxHeight = height;
                }
            });

            headers.forEach(function(header) {
                header.style.height = maxHeight - 48 + 'px';
            });
            }
        }
        equalizeHeights();
  });
</script>
